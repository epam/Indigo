    <extension point="org.knime.workbench.repository.categories">
      <category description="Indigo 2" icon="icons/indigo.png" level-id="indigo 2" name="Indigo 2" path="/community"/>
    </extension>
    <extension point="org.knime.workbench.repository.categories">
      <category
            description="Molecule Nodes"
            level-id="molecule"
            name="Molecule Nodes"
            path="/community/indigo 2"/>
      <category
            after="molecule"
            description="Reaction Nodes"
            level-id="reaction"
            name="Reaction Nodes"
            path="/community/indigo 2">
      </category>
      <category
            after="reaction"
            description="Match and Similarity"
            level-id="structure"
            name="Match and Similarity"
            path="/community/indigo 2">
      </category>
      <category
            after="structure"
            description="Combinatorial Chemistry"
            icon="icons/groups/combichem-group.png"
            level-id="combinatorial_chemistry"
            name="Combinatorial Chemistry"
            path="/community/indigo 2">
      </category>
      <category
            after="combinatorial_chemistry"
            description="Manipulators"
            icon="icons/groups/manipulators.png"
            level-id="manipulators"
            name="Manipulators"
            path="/community/indigo 2">
      </category>
      <category
            after="manipulators"
            description="Properties"
            icon="icons/groups/properties.png"
            level-id="properties"
            name="Properties"
            path="/community/indigo 2">
      </category>
    </extension>
    <extension point="org.eclipse.equinox.preferences.preferences">
       <initializer class="com.epam.indigo.knime.plugin.IndigoPreferenceInitializer"/>
    </extension>
    <extension point="org.eclipse.ui.preferencePages">
      <page category="org.knime.chem.preferences" class="com.epam.indigo.knime.plugin.IndigoPreferencePage" id="com.epam.indigo.knime.preferences" name="Indigo"/>
    </extension>
    <extension point="org.knime.product.splashExtension">
       <splashExtension icon="icons/indigo-splash.png" id="indigo" tooltip="Indigo"/>
    </extension>
    <extension
          point="org.knime.core.DataValueRenderer">
       <dataValue
             valueClass="org.knime.chem.types.MolValue">
          <renderer
                rendererFactoryClass="com.epam.indigo.knime.cell.IndigoDataValueRenderer$Factory"
                suggestAsDefault="true">
