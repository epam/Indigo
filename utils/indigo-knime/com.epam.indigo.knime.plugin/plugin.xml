          <renderer
                rendererFactoryClass="org.knime.chem.types.MolValueRenderer$Factory"
                suggestAsDefault="false">
          </renderer>
       </dataValue>
       <dataValue
             valueClass="org.knime.chem.types.SdfValue">
          <renderer
                rendererFactoryClass="com.epam.indigo.knime.cell.IndigoDataValueRenderer$Factory"
                suggestAsDefault="true">
          </renderer>
          <renderer
                rendererFactoryClass="org.knime.chem.types.SdfValueRenderer$Factory"
